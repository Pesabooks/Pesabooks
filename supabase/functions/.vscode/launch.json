  "version": "0.2.0",
  "configurations": [
    {
      "name": "Supabase-functions",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}\\supabase",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-env",
        "--import-map",
        "${fileDirname}\\..\\import_map.json",
        "-A",
        "${fileDirname}\\${fileBasename}"
      ],
      "attachSimplePort": 9229,
      "outputCapture": "std"
    }
  ]
}
